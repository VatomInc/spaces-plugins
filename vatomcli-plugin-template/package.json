    "babel-loader": "^8.2.4",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.21.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "vatom-spaces-plugins": "^1.1.2"
  }
}
